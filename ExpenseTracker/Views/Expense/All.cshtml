@model List<ExpenseDto>
@if(Model.Count==0)
{
    <h3>No expenses.</h3>
    <h4>Add!</h4>
}
@if(Model.Count>0)
{
     <h2>Your expenses</h2>
    @foreach(var m in Model)
    {
        <div class="card" style="width: 18rem;">
            <img src="..." class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">Expense type-@m.Type</h5>
                <p class="card-text">Expense summary @m.Summary</p>
                <p class="card-text">Expense date @m.Date</p>
                <p class="card-text">Expense ammount @m.Amount</p>
            </div>
        </div>
    }
}
